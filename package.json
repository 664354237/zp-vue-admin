    "core-js": "^3.30.2",
    "echarts": "^5.4.2",
    "element-ui": "2.13.2",
    "html2canvas": "^1.4.1",
    "js-cookie": "2.2.0",
    "jspdf": "^2.5.1",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "pinyin": "^3.0.0-alpha.5",
    "swiper": "^10.1.0",
    "vue": "2.6.14",
    "vue-awesome-swiper": "^2.6.7",
